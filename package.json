{
  "name": "HS-Main",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "==================== CORE": "====================",
    "clean": "rimraf data/ build/ .tmp/ 'source/.assets/!(fonts|images)**'",
    "==================== DATA": "====================",
    "data": "bundle exec middleman contentful",
    "data:live": "CONTENTFUL_ENV=live bundle exec middleman contentful",
    "data:live:clean": "npm run clean && CONTENTFUL_ENV=live bundle exec middleman contentful",
    "data:preview": "CONTENTFUL_ENV=preview bundle exec middleman contentful",
    "data:preview:clean": "npm run clean && CONTENTFUL_ENV=preview bundle exec middleman contentful",
    "==================== SCRIPTS": "====================",
    "before-build": "node ./lib/js/_before-build",
    "==================== GULP": "====================",
    "prepipe:prod": "gulp prod --GULP_ENV=production",
    "epipe:dev": "gulp --GULP_ENV=development",
    "images": "gulp images",
    "css:dev": "gulp css --GULP_ENV=development",
    "css:prod": "gulp css --GULP_ENV=production",
    "js:dev": "gulp js --GULP_ENV=development",
    "js:prod": "gulp js --GULP_ENV=production",
    "==================== BUILDS": "====================",
    "prod": "npm run prepipe:prod && MM_ENV=prod bundle exec middleman build -e prod",
    "prod:deploy": "npm run clean && npm run data:live && npm run prepipe:prod && MM_ENV=prod bundle exec middleman build --verbose -e prod",
    "preview": "npm run prepipe:prod && MM_ENV=preview bundle exec middleman build -e preview",
    "preview:deploy": "npm run clean && npm run data:preview && npm run before-build && npm run prepipe:prod && MM_ENV=preview bundle exec middleman build --verbose -e preview",
    "dev": "MM_ENV=dev bundle exec middleman server",
    "dev:clean": "npm run clean && npm run data:preview && npm run before-build && MM_ENV=dev bundle exec middleman server",
    "dev:clean:prod": "npm run clean && npm run data:live && npm run before-build && MM_ENV=dev bundle exec middleman server",
    "==================== POST BUILD": "====================",
    "post:prod": "NODE_ENV=prod node lib/node/_scripts.js",
    "post:preview": "NODE_ENV=preview node lib/node/_scripts.js"
  },
  "keywords": [],
  "author": "@LukeBoga",
  "license": "ISC",
  "devDependencies": {
    "algoliasearch": "^3.30.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.18.8",
    "fs-extra": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-favicons": "^2.2.7",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-sass-variables": "^1.1.1",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-svgmin": "^1.2.4",
    "gulp-util": "^3.0.8",
    "js-yaml": "^3.10.0",
    "lazypipe": "^1.0.1",
    "natives": "^1.1.6",
    "netlify-lambda": "^1.4.2",
    "postcss-uncss": "^0.15.0",
    "rimraf": "^2.6.1",
    "run-sequence": "^1.2.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "uncss": "^0.15.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0",
    "webpack-stream": "^4.0.0"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "blazy": "^1.8.2",
    "chart.js": "^2.8.0",
    "chartjs-chart-radial-gauge": "^1.0.3",
    "chartjs-plugin-annotation": "github:chartjs/chartjs-plugin-annotation",
    "chartjs-plugin-datalabels": "^0.4.0",
    "chartjs-plugin-deferred": "^1.0.1",
    "chartjs-plugin-labels": "^1.1.0",
    "chroma-js": "^1.4.0",
    "core-js": "^3.0.1",
    "flickity": "^2.2.0",
    "gulp-csso": "^3.0.1",
    "infinite-scroll": "^3.0.2",
    "instantsearch.js": "^2.10.2",
    "lodash": "^4.17.5",
    "lodash.chunk": "^4.2.0",
    "normalize.css": "^5.0.0",
    "picturefill": "^3.0.2",
    "plyr": "^3.5.6",
    "promise-polyfill": "^6.0.2",
    "smooth-scroll": "^12.1.5",
    "svgxuse": "^1.2.6",
    "whatwg-fetch": "^2.0.3"
  }
}
