{
  "name": "HS-Main",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf data/ .tmp/ build/",
    "data": "bundle exec middleman contentful",
    "data:live": "CONTENTFUL_ENV=live bundle exec middleman contentful",
    "data:test": "CONTENTFUL_ENV=preview bundle exec middleman contentful",
    "epipe:pre": "gulp assets --GULP_ENV=production",
    "epipe:build": "gulp build --GULP_ENV=production",
    "epipe:dev": "gulp --GULP_ENV=development",
    "prod": "yarn run epipe:pre && bundle exec middleman build -e prod",
    "prod:clean": "yarn run clean && yarn run epipe:pre && yarn run data:live && bundle exec middleman build -e prod",
    "prod:deploy": "yarn run epipe:pre && yarn run data:live && bundle exec middleman build -e prod",
    "test": "yarn run epipe:pre && bundle exec middleman build -e test",
    "test:clean": "yarn run clean && yarn run epipe:pre && yarn run data:test && bundle exec middleman build -e test",
    "test:deploy": "yarn run epipe:pre && yarn run data:test && bundle exec middleman build -e test",
    "dev": "bundle exec middleman server",
    "dev:clean": "yarn run clean && yarn run data:test && bundle exec middleman server",
    "dev:poll": "bundle exec middleman server --watcher-force-polling"
  },
  "keywords": [],
  "author": "@LukeBoga",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.8",
    "browserify": "^14.1.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.10.0",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^6.3.0",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.1.0",
    "gulp-streamify": "^1.0.2",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-touch": "^1.0.1",
    "gulp-util": "^3.0.8",
    "lazypipe": "^1.0.1",
    "node-sass": "^4.5.0",
    "postcss-reporter": "^3.0.0",
    "postcss-scss": "^0.4.0",
    "purify-css": "^1.2.5",
    "rimraf": "^2.6.1",
    "run-sequence": "^1.2.2",
    "stylelint": "^7.8.0",
    "stylelint-scss": "^1.4.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "picturefill": "^3.0.2",
    "smooth-scroll": "cferdinandi/smooth-scroll",
    "swiper": "^3.4.2"
  }
}
