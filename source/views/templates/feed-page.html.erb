<%
entryData = entry_data
feedData = alt_data[:feed_data]

title = "Title"

if siteCategories(:top_all).include?(feedData[:feed_category])
  siteCategories(:top_all).each do |cat|
    if feedData[:feed_category] == cat
      title = "#{ cat.capitalize } / Page #{ feedData[:feed_page] }"

    end
  end
else
  data.hs.theme_page.each do |id, page|
    if feedData[:feed_category] == page[:theme]
      title = "#{ page[:title] } / Page #{ feedData[:feed_page] }"
    end
  end
end
%>

<%# Banner %>
<%= partial "views/partials/banner/landing-page",
  locals: { local_data: {
    entry_data: {
      TYPE: "landing_page",
      title: title,
      banner_image: {
        url: "url",
        alt: "alt",
        focus: "center"
      }
    }
  }} %>

<%# Feed %>
<%= partial "views/partials/panel/feed",
  locals: { local_data: {
    entry_data: entryData,
    alt_feed_data: feedData }} %>
