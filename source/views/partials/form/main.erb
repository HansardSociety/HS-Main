<%
formData = local_data[:entry_data]

isSubscriptionForm = formData[:TYPE] == "form"
isPaymentForm = formData[:TYPE] == "product"

formData.merge!({
  is_modal: local_data[:modal]
}.compact)
%>

<% if isSubscriptionForm %>
  <%= partial "views/partials/form/subscription",
    locals: { form_data: formData } %>
<% elsif isPaymentForm %>
  <%= partial "views/partials/form/checkout",
    locals: { form_data: formData } %>
<% end %>
