<%
formData = local_data[:entry_data]

isNetlifyForm = formData[:TYPE] == "form"
isPaymentForm = formData[:TYPE] == "product"

formData.merge!({
  is_modal: local_data[:modal]
}.compact)
%>

<% if isNetlifyForm %>
  <%= partial "views/partials/form/fields-netlify",
    locals: { form_data: formData } %>
<% elsif isPaymentForm %>

<% end %>
