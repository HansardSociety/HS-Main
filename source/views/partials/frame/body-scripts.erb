<%
isLandingPage = @entry[:TYPE] == "landing_page"
featured = @entry[:featured]
%>

<%# Search templates %>
<%= partial "views/partials/search/template-scripts" %>

<%# Core JS %>
<script src="<%= assetsDir %><%= config[:JS_VENDOR] %>"></script>
<script src="<%= assetsDir %><%= config[:JS_APP] %>"></script>

<% if @entry[:TYPE] == "theme_page" || (@entry[:category] == "publications" && @entry[:index_page]) %>
  <script src="<%= assetsDir %><%= config[:JS_SEARCH] %>"></script>
<% end %>

<%# Chart JS %>
<% if isLandingPage && @entry[:panels].any?{|panel| panel[:TYPE] == "panel_chart"} %>
  <script src="<%= assetsDir %><%= config[:JS_CHART] %>"></script>
<% end %>
